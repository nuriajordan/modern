\n    </div>\n</template>\n\n<script>\nexport default {\n  name: \"ImagesView\",\n  \n  data(){\n    return{\n        selectedDay:null\n    }\n  },\n\n  computed:{\n    cards(){\n        console.log(this.$store.getters.cards)\n        if (this.selectedDay!=null){\n        return this.$store.getters.cards.filter(n=> n.map==this.selectedDay)\n        }else{\n            return this.$store.getters.cards\n        }\n    }, \n    days(){\n        console.log(this.$store.getters.days)\n        return this.$store.getters.days\n    },\n  },\n  methods:{\n    getDay(id){\n        console.log(id);\n        this.selectedDay = id\n    }\n  }\n};\n</script>\n\n<style scoped lang=\"scss\">\n\np{\n    color: black;\n    margin: 0;\n}\n\n.flex button{\n    color: grey;\n    font-size: 15px;\n}\n\n.flex .dia button.active{\n    color: black;\n}\n\n.flex button:hover{\n    color: black;\n}\n\n.days{\n    background-color: white;\n    padding: 0 40px;\n}\n\n.flex{\n    display: flex;\n   \n}\n\nbutton{\n    border-style: none;\n    background-color: transparent;\n}\n\n.images{\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center;\n}\n\n.images .image{\n    padding: 150px 115px;\n    background-color: aqua;\n    margin: 20px;\n}\n\n.slide{\n    border-radius: 2em;\n    border: 1px solid black;\n}\n\n</style>"]},"metadata":{},"sourceType":"module"}