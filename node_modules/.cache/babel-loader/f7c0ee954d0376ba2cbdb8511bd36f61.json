ntent":["<template>\n  <div>\n    <div class=\"menu\">\n      <div class=\"buscador\">\n        <p id=\"day\">Filter by: DAY</p>\n        <p id=\"title\">Insights of humans</p>\n        <input v-model=\"message\" placeholder=\"Search\" />\n      </div>\n      <div class=\"logos\">\n        <router-link to=\"/cards\" class=\"logo\">\n          <img src=\"..//..//public/icons/cardsfill.svg\" alt=\"\" />\n        </router-link>\n        <router-link to=\"/notebook\" class=\"logo\">\n          <img src=\"..//..//public/icons/notebook.svg\" alt=\"\" />\n        </router-link>\n        <router-link to=\"/images\" class=\"logo\">\n          <img src=\"..//..//public/icons/photo.svg\" alt=\"\" />\n        </router-link>\n        <router-link to=\"/day\" class=\"logo\">\n          <img src=\"..//..//public/icons/experience.svg\" alt=\"\" />\n        </router-link>\n        <router-link to=\"/save\" class=\"logo\">\n          <img src=\"..//..//public/icons/save.svg\" alt=\"\" />\n        </router-link>\n      </div>\n    </div>\n    <div class=\"filter\">\n      <button>memories</button>\n      <button>experience</button>\n      <button>idea</button>\n      <button>online</button>\n      <button>anecdote</button>\n    </div>\n    <div class=\"days\">\n      <div class=\"flex\">\n        <div class=\"slide\">\n          <div v-for=\"d in days\" :key=\"d._id\" class=\"dia\">\n            <button\n              @click=\"filter(d._id)\"\n              :class=\"{ active: filterId == d._id }\"\n              id=\"select\"\n            >\n              {{ d.point }}\n            </button>\n          </div>\n        </div>\n        <div class=\"cards\">\n          <div\n            v-for=\"d in cards\"\n            :key=\"d._id\"\n            class=\"card relative\"\n            @click=\"goToCard(d)\"\n          >\n            <div class=\"absolute\">\n              <button class=\"save\">\n                <img src=\"..//..//public/icons/save.svg\" alt=\"\" />\n              </button>\n            </div>\n            <button id=\"border\">\n              <img :src=\"'..//..//public/cards/' + d.frontcover\" alt=\"\" />\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <button @click=\"click = !click\" class=\"\">\n      <div v-if=\"click\">\n        <p>\n          WildTech is a design research project aimed at investigating how\n          future technology might support human-nature interactions that are\n          profoundly joyful, caring, and fun, led by the Gamification Group\n          (Tampere University) in collaboration with Escola Universitària ERAM\n          (Universitat de Girona). It is funded by Finland's Academy of Science\n          through the UNITE Flagship. Using experimental co-design methods, and\n          displacing our design & research practice into the wilderness, we\n          imagine how tech can contribute to keeping us connected with (and\n          through) nature.\n        </p>\n        <p>\n          This interactive piece was designed and developed by a team of\n          students at Escola Universitària ERAM: Ester Montero (UX/UI design)\n          Núria Jordán (Developer). They were supported by Jordi Márquez and\n          Ferran Altarriba Bertran.\n        </p>\n      </div>\n      <div class=\"calendar\" v-else>\n        <img src=\"..//..//public/icons/arrowup.svg\" alt=\"\" />\n        <button>About us</button>\n      </div>\n    </button>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"CardsView\",\n\n  data() {\n    return {\n      filterId: null,\n      hover: false,\n    };\n  },\n  beforeMount() {\n    if (localStorage.getItem(\"savedData\")) {\n      this.saved = JSON.parse(localStorage.getItem(\"savedData\"));\n    }\n  },\n  computed: {\n    cards() {\n      console.log(this.$store.getters.cards);\n      if (this.filterId != null) {\n        return this.$store.getters.cards.filter((n) => n.map == this.filterId);\n      } else {\n        /* if (this.selectedMemories!=null){\n        return this.$store.getters.cards.filter(n=> n.type==this.selectedMemories)\n        } */\n        return this.$store.getters.cards;\n      }\n    },\n    days() {\n      console.log(this.$store.getters.days);\n      return this.$store.getters.days;\n    },\n  },\n  methods: {\n    filter(id) {\n      this.filterId = this.filterId == id ? null : id;\n    },\n    /* getDay(id){\n        this.selectedDay = id\n    }, */\n    /* getMemories(id){\n        this.selectedMemories = id\n    },\n    getExperience(id){\n        this.selectedExperience = id\n    },\n    getIdea(id){\n        this.selectedIdea = id\n    },\n    getOnline(id){\n        this.selectedOnline = id\n    },\n    getAnecdote(id){\n        this.selectedAnecdote = id\n    }, */\n    goToCard(d) {\n      this.$router.push({ path: \"/card/\" + d._id });\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\nbody {\n  background-color: #0c1028;\n}\n\np {\n  color: white;\n  margin: 0;\n}\n\nbutton {\n  border-style: none;\n  background-color: transparent;\n  color: white;\n}\n\nimg {\n  border-radius: 7%;\n  width: auto;\n  height: 260px;\n}\n\n.menu {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n  background-color: #0c1028;\n}\n\n.menu .buscador {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: center;\n  align-content: center;\n  margin-left: 60px;\n  margin-top: 30px;\n}\n\n.menu p {\n  margin: 0;\n}\n\n.menu input {\n  width: 420px;\n  height: 35px;\n  border: 1px solid white;\n  border-radius: 30px;\n  background-color: #0c1028;\n  box-shadow: 1px 1px 6px #0c1028;\n  margin-left: 190px;\n  font-size: 14px;\n  padding-left: 25px;\n  color: white;\n}\n\n.menu .logos .logo {\n  padding: 10px;\n  color: black;\n}\n\n.menu img {\n  width: 40px;\n  height: 40px;\n}\n\n.menu .logos {\n  margin-right: 60px;\n  margin-top: 30px;\n}\n\n.menu .buscador #title {\n  font-size: 20px;\n  margin-left: 40px;\n}\n\n.menu .buscador #day {\n  border-radius: 50%;\n  padding: 15px;\n  border-color: white;\n  border-width: 1px;\n  border-style: solid;\n  width: 30px;\n  height: 30px;\n  font-size: 9px;\n}\n\n.flex button {\n  color: grey;\n  font-size: 15px;\n}\n\n.days {\n  padding: 0 40px;\n  background-color: #0c1028;\n}\n\n.dia {\n  padding: 5px;\n  width: 38px;\n  height: 38px;\n  margin: 0;\n}\n\n.dia #select {\n  width: 38px;\n  height: 38px;\n}\n\n.flex {\n  display: flex;\n}\n\n.flex .dia button.active {\n  color: black;\n  background-color: white;\n  border-radius: 50px;\n}\n\n.flex .dia button.active:hover {\n  color: black;\n}\n\n.flex .dia button:hover {\n  border-color: white;\n  color: black;\n  border: 1px;\n  border-style: solid;\n  border-radius: 50px;\n}\n\n.dia #select:hover {\n  color: black;\n  color: white;\n  border-color: white;\n}\n\n.cards {\n  display: flex;\n  flex-wrap: wrap;\n  height: 650px;\n  overflow-y: scroll;\n  scroll-behavior: smooth;\n  justify-content: space-between;\n  width: 100%;\n  .card {\n    padding-left: 20px;\n    padding-right: 20px;\n    padding-top: 11px;\n    padding-bottom: 30px;\n    &:hover {\n      .save {\n        display: block;\n      }\n    }\n    .save {\n      display: none;\n    }\n  }\n}\n\n::-webkit-scrollbar {\n  width: 0;\n  scrollbar-color: #0c1028;\n}\n\n.filter {\n  text-align: left;\n  padding-left: 175px;\n  background-color: #0c1028;\n}\n\n.filter button {\n  font-size: 13px;\n}\n\n.filter button:hover {\n  text-decoration: underline;\n}\n\n.days .slide {\n  border-radius: 2em;\n  border: 1px solid white;\n  margin-left: 25px;\n  height: 650px;\n  overflow-y: scroll;\n  scroll-behavior: smooth;\n}\n\n::-webkit-scrollbar {\n  width: 5px;\n}\n\n.slide .active {\n  width: 100px;\n}\n\n.relative {\n  position: relative;\n}\n\n.absolute {\n  position: absolute;\n  left: 40%;\n  top: 90%;\n}\n\n.absolute img {\n  width: 30px;\n  height: 30px;\n}\n\n.fixed {\n  position: fixed;\n  bottom: 0;\n  right: 0;\n  width: 100%;\n  height: 60p