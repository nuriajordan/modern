tters.days)\n        return this.$store.getters.days\n    },\n  },\n  methods:{\n    filter(id){\n       this.filterId = this.filterId==id?null:id;\n    }, \n    /* getDay(id){\n        this.selectedDay = id\n    }, */ \n    /* getMemories(id){\n        this.selectedMemories = id\n    },\n    getExperience(id){\n        this.selectedExperience = id\n    },\n    getIdea(id){\n        this.selectedIdea = id\n    },\n    getOnline(id){\n        this.selectedOnline = id\n    },\n    getAnecdote(id){\n        this.selectedAnecdote = id\n    }, */\n    goToCard(d){\n        this.$router.push({path:'/card/'+d._id})\n    }\n  }\n};\n</script>\n\n<style scoped lang=\"scss\">\n\nbody{\n   background-color: #0C1028;\n}\n\np{\n    color: black;\n    margin: 0;\n}\n\n.flex button{\n    color: grey;\n    font-size: 15px;\n}\n\n.flex .dia button.active{\n    color: black;\n}\n\n.flex button:hover{\n    color: black;\n}\n\n.days{\n    padding: 0 40px;\n}\n\n.flex{\n    display: flex;\n}\n\nbutton{\n    border-style: none;\n    background-color: transparent;\n}\n\n.cards{\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center;\n    .card{\n        &:hover{\n            .save{\n                display:block;\n            }\n        }\n        .save{\n            display:none;\n        }\n    }\n}\n\nimg{\n    border-radius: 7%;\n    width: auto;\n    height: 260px;\n}\n\nimg:hover{\n    box-shadow: 2px 2px 25px grey;\n}\n\n.filter button:hover{\n    text-decoration: underline;\n}\n\n.slide{\n    border-radius: 2em;\n    border: 1px solid black;\n}\n\n.dia button{\n    font-size: 16px;\n\n}\n\n.relative{\n    position: relative;\n}\n\n.absolute{\n    position: absolute;\n    right: 0; width: 100%; left: 35%;\n    top: 85%; \n}\n\nbutton{\n    color: white;\n}\n\n</style>"]},"metadata":{},"sourceType":"module"}