erflow-y: scroll;\n    scroll-behavior: smooth;\n}\n\n::-webkit-scrollbar {\n  width: 5px;\n}\n\n\n.days .slide{\n    border-radius: 2em;\n    border: 1px solid black;\n    margin-top: 25px;\n    margin-right: 50px;\n    height: 650px;\n    overflow-y: scroll;\n    scroll-behavior: smooth;\n}\n\n.days .dia{\n    padding: 5px;\n}\n\n.days .dia #selected{\n    width: 38px;\n    height: 38px;\n}\n\n.days .flex .dia button.active{\n    color: black;\n    background-color: white;\n    border-radius: 50px;\n}\n\n.days .flex .dia button:hover {\n    color: white;\n    border-color: white;\n    border: 1px;\n    border-style: solid;\n    border-radius: 50px;\n}\n\n.days .slide{\n    border-radius: 2em;\n    border: 1px solid white;\n    margin-left: 25px;\n}\n\n.days .quotes #espai{\n    margin-top: 25px;\n    font-weight: 550;\n}\n\n.days .fixed{\n    background-color: #0C1028;\n    position: fixed;\n    bottom: 0;\n    right: 0;\n    width: 100%;\n    height: 60px;\n}\n\n.days .fixed button{\n    margin-top: 10px;\n    color: white;\n    float: right;\n    font-size: 10px;\n    margin-right: -32px;\n    margin-top: 22px;\n}\n\n.days .fixed img{\n    width: 11px;\n    height: 7px;\n    float: right;\n    margin-right: 70px;\n    margin-top: 15px;\n}\n\n.days .relative{\n    position: relative;\n        &:hover{\n        .save{\n            display:block;\n            }\n        }\n        .save{\n            display:none;\n        }\n    width: 1000px;\n}\n\n.days .absolute{\n    position: absolute;\n    right: 5%;\n    top: 40%; \n}\n\n.days .absolute img{\n    width: 30px;\n    height: 30px;\n}\n\n.menu {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: space-between;\n    background-color: #0C1028;\n}\n\n.menu .buscador{\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center;\n    align-content: center;\n    margin-left: 60px;\n    margin-top: 30px;\n}\n\n.menu p{\n    margin: 0;\n}\n\n.menu input{\n    width: 420px;\n    height: 35px;\n  