{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"NotebookView\",\n  data() {\n    return {\n      filterId: null\n    };\n  },\n  beforeMount() {\n    if (localStorage.getItem('savedData')) {\n      this.saved = JSON.parse(localStorage.getItem('savedData'));\n    }\n  },\n  computed: {\n    notebook() {\n      console.log(this.$store.getters.notebook);\n      if (this.filterId != null) {\n        return this.$store.getters.notebook.filter(n => n.Day == this.filterId);\n      } else {\n        return this.$store.getters.notebook;\n      }\n    },\n    days() {\n      console.log(this.$store.getters.days);\n      return this.$store.getters.days;\n    }\n  },\n  methods: {\n    saveCard(text) {\n      if (localStorage.getItem('savedData')) {\n        this.saved = JSON.parse(localStorage.getItem('savedData'));\n      }\n      if (this.saved.text.includes(text._id)) {\n        console.log(this.saved.text.indexOf(text._id));\n        this.saved.text.splice(this.saved.text.indexOf(text._id), 1);\n      } else {\n        this.saved.text.push(text._id);\n      }\n      localStorage.setItem('savedData', JSON.stringify(this.saved));\n    },\n    changeId() {\n      this.id = this.days[this.Day]._id;\n      this.Day++;\n    },\n    filter(id) {\n      this.filterId = this.filterId == id ? null : id;\n    }\n  }\n};","map":{"version":3,"mappings":";AA6CA;EACAA;EAEAC;IACA;MACAC;IACA;EACA;EAEAC;IACA;MACA;IACA;EACA;EACAC;IACAC;MACAC;MACA;QACA;MACA;QACA;MACA;IACA;IACAC;MACAD;MACA;IACA;EACA;EACAE;IACAC;MACA;QACA;MACA;MACA;QACAH;QACA;MACA;QACA;MACA;MACAI;IACA;IACAC;MAEA;MACA;IACA;IAEAC;MACA;IACA;EACA;AACA","names":["name","data","filterId","beforeMount","computed","notebook","console","days","methods","saveCard","localStorage","changeId","filter"],"sourceRoot":"src/components","sources":["NotebookWorld.vue"],"sourcesContent":["<template>\n    <div>\n        <div class=\"menu\">\n        <div class=\"buscador\">\n            <p id=\"day\">Filter by: DAY</p>\n            <p id=\"title\">Insights of humans</p>\n            <input v-model=\"message\" placeholder=\"Search\">\n        </div>\n        <div class=\"logos\">\n            <router-link to=\"/cards\" class=\"logo\"> <img src=\"..//..//public/icons/cards.svg\" alt=\"\"> </router-link>\n            <router-link to=\"/notebook\" class=\"logo\"> <img src=\"..//..//public/icons/llibrefill.svg\" alt=\"\"> </router-link>\n            <router-link to=\"/images\" class=\"logo\"> <img src=\"..//..//public/icons/photofill.svg\" alt=\"\"> </router-link>\n            <router-link to=\"/day\" class=\"logo\"> <img src=\"..//..//public/icons/experience.svg\" alt=\"\"> </router-link>\n            <router-link to=\"/save\" class=\"logo\"> <img src=\"..//..//public/icons/save.svg\" alt=\"\"> </router-link>\n        </div>\n    </div>\n        <div class=\"days\">\n            <div class=\"flex\">\n                <div class=\"slide\">\n                    <div v-for=\"d in days\" :key=\"d._id\" class=\"dia\">\n                        <button @click=\"filter(d._id)\" :class=\"{'active':filterId==d._id}\" id=\"selected\">{{d.point}}</button>\n                    </div>\n                </div>\n                <div class=\"quotes\">\n                    <div v-for=\"d in notebook\" :key=\"d._id\" class=\"relative\">\n                        <p id=\"espai\">{{d.Point}}</p>\n                        <p>{{d.Text}}</p>\n                        <div class=\"absolute\">\n                            <button class=\"save\"><img src=\"..//..//public/icons/save.svg\" alt=\"\"></button>\n                        </div>\n                    </div>\n                    \n                <p></p>\n                </div>\n                \n            </div>\n            <div class=\"fixed\">\n                <img src=\"..//..//public/icons/arrowup.svg\" alt=\"\">\n                <button>About us</button>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nexport