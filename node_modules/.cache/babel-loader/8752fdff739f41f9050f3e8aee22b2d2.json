ck\"\n        />\n        <a>About us</a>\n      </router-link>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"NotebookView\",\n\n  data() {\n    return {\n      filterId: null,\n      click: null,\n    };\n  },\n\n  beforeMount() {\n    if (localStorage.getItem(\"savedData\")) {\n      this.saved = JSON.parse(localStorage.getItem(\"savedData\"));\n    }\n  },\n  computed: {\n    notebook() {\n      console.log(this.$store.getters.notebook);\n      if (this.filterId != null) {\n        return this.$store.getters.notebook.filter(\n          (n) => n.Day == this.filterId\n        );\n      } else {\n        return this.$store.getters.notebook;\n      }\n    },\n    days() {\n      console.log(this.$store.getters.days);\n      return this.$store.getters.days;\n    },\n  },\n  methods: {\n    saveCard(text) {\n      if (localStorage.getItem(\"savedData\")) {\n        this.saved = JSON.parse(localStorage.getItem(\"savedData\"));\n      }\n      if (this.saved.text.includes(text._id)) {\n        console.log(this.saved.text.indexOf(text._id));\n        this.saved.text.splice(this.saved.text.indexOf(text._id), 1);\n      } else {\n        this.saved.text.push(text._id);\n      }\n      localStorage.setItem(\"savedData\", JSON.stringify(this.saved));\n    },\n    changeId() {\n      this.id = this.days[this.Day]._id;\n      this.Day++;\n    },\n\n    filter(id) {\n      this.filterId = this.filterId == id ? null : id;\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\nbody {\n  background-color: #0c1028;\n}\n\np {\n  color: white;\n  margin: 0;\n}\n\n.quotes p {\n  width: 800px;\n  text-align: left;\n}\n\n.days {\n  margin-left: 40px;\n}\n\n.days .flex button {\n  color: grey;\n  font-size: 15px;\n}\n\n.days .flex .dia button.active {\n  color: black;\n}\n\n.days .flex button:hover {\n  color: black;\n}\n\n.days .flex .days {\n  padding: 0 40px;\n}\n\n.days .flex {\n  display: flex;\n}\n\n.days button {\n  border-style: none;\n  backgr