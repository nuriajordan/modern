rm:null,\n      canvas:null,\n      c:null,\n       keys : {\n        right: {\n            pressed: false\n        },\n        left: {\n            pressed: false\n        }\n       },\n       gravity: 0.5\n\n    }\n  },\n  mounted(){\n\n\n\nthis.canvas = this.$refs.canvas\n\nthis.c = this.canvas.getContext('2d')\n\nthis.canvas.width = window.innerWidth\nthis.canvas.height = window.innerHeight\nthis.player=new Player(this.canvas,this.c,this.gravity);\nthis.platform = new Platform(this.c)\n\nthis.animate()\n\naddEventListener('keydown', ({ keyCode }) => {\n\n    switch (keyCode){\n        case 37: \n            console.log('left')\n            this.keys.left.pressed = true\n            break\n\n        case 40: \n            console.log('down')\n            break\n\n        case 38: \n            console.log('up')\n            this.player.velocity.y -= 20\n            break\n\n        case 39: \n            console.log('right')\n            this.keys.right.pressed = true\n            break\n    }\n})\n\naddEventListener('keyup', ({ keyCode }) => {\n\n    switch (keyCode){\n        case 37: \n            console.log('left')\n            this.keys.left.pressed = false\n            break\n\n        case 40: \n            console.log('down')\n            break\n\n        case 38: \n            console.log('up')\n            this.player.velocity.y -= 20\n            break\n\n        case 39: \n            console.log('right')\n            this.keys.right.pressed = false\n            break\n    }\n})\n  },\n  methods:{\n    onClickButton () {\n      this.$emit('goInfo','1')\n      this.$router.push({path:'/information'})\n    }, \n    changeId(){\n      this.id=this.days[this.dia]._id\n      this.dia++;\n    },\n    animate(){\n    requestAnimationFrame(this.animate)\n    this.c.clearRect(0,0, this.canvas.width, this.canvas.height)\n    this.player.update()\n    platform.draw()\n\n    if(this.keys.right.pressed && this.player.position.x < 400){\n        this.player.velocity.x = 5\n    } else if (this.keys.left.pressed && this.player.position.x > 100){\n        this.player.velocity.x = -5\n    } else {\n        this.player.velocity.x = 0\n\n        if(this.keys.right.pressed){\n            platform.position.x -= 5\n        } else if (this.keys.left.pressed){\n            platform.position.x += 5\n        }\n    }\n}\n\n  }\n};\n</script>\n\n<style scoped lang=\"scss\">\nbody{\n   background-color: #0C1028;\n}\n\np{\n  margin: 0;\n  padding: 0;\n}\n\n.menu {\n  background-color: #0C1028;\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n  align-items: center;\n  margin-top: 30px;\n}\n\n.menu .logos{\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n  margin-right: 60px;\n}\n\n.menu img{\n  width: 40px;\n  height: 40px;\n  padding: 10px;\n}\n\n.menu p{\n  color: black;\n  font-size: 12px;\n}\n\n.menu button{\n  border-radius: 50%;\n  padding: 15px;\n  border-color: black;\n  border-width: 1px;\n  width: 62px;\n  height: 62px;\n}\n\n.menu button:hover{\n  background: black;\n}\n\n.menu button:hover p{\n  color: white;\n}\n\n\n.menu .flex{\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: center;\n  align-items: center;\n  margin-left: 60px;\n}\n\n.menu .location p{\n  text-align: left;\n  margin-left: 30px;\n}\n\n.center img{\n    width: 95px;\n    height: 170px;\n}\n\n.center {\n    font-size: 50px;\n    padding: 70px;\n}\n\n.center p{\n    color: black;\n}\n\n.footer {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center;\n}\n\n.footer p{\n    margin: 20px;\n}\n\n.footer a{\n    margin: 20px;\n    text-align: right;\n}\n\n\n\n</style>"]},"metadata":{},"sourceType":"module"}