_id)) {\n        console.log(this.saved.cards.indexOf(card._id));\n        this.saved.cards.splice(this.saved.cards.indexOf(card._id), 1);\n      } else {\n        this.saved.cards.push(card._id);\n      }\n      localStorage.setItem(\"savedData\", JSON.stringify(this.saved));\n    },\n    getDay(id) {\n      this.selectedDay = id;\n    },\n    goToCard(d) {\n      this.$router.push({ path: \"/card/\" + d._id });\n    },\n    onClickButton() {\n      this.$emit(\"goInfo\", \"1\");\n      this.$router.push({ path: \"/day\" });\n    },\n    goToImage() {\n      this.currentImage = this.$store.getters.images\n        .map((d) => {\n          return d._id;\n        })\n        .indexOf(this.$route.params.id);\n      console.log(\"curr img\", this.currentImage);\n      this.$router.push({\n        path:\n          \"/imageInfo/\" + this.$store.getters.images[this.currentImage + 1]._id,\n      });\n    },\n    goToPrevImage() {\n      this.currentImage = this.$store.getters.images\n        .map((d) => {\n          return d._id;\n        })\n        .indexOf(this.$route.params.id);\n      console.log(\"curr img\", this.currentImage);\n      this.$router.push({\n        path:\n          \"/imageInfo/\" + this.$store.getters.images[this.currentImage - 1]._id,\n      });\n    },\n  },\n};\n</script>\n\n<style scoped lang=\"scss\">\n.menu {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n  align-items: center;\n  margin-top: 30px;\n}\n\n.menu .logos {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n  margin-right: 60px;\n}\n\n.menu p {\n  color: white;\n}\n\n.menu .logo {\n  color: black;\n}\n\n.menu .calendar {\n  border-radius: 50%;\n  padding: 15px;\n  border-color: white;\n  border-width: 1px;\n  border-style: solid;\n  width: 30px;\n  height: 30px;\n}\n\n.menu .calendar p {\n  font-size: 12px;\n}\n\n.menu p {\n  margin: 0;\n  padding: 0;\n  text-align: center;\n  font-size: 16px;\n}\n\n.flex {\n  display: flex;\n  flex-w